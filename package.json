{
    "name": "unirep-social",
    "version": "0.0.1",
    "private": true,
    "description": "",
    "main": "index.ts",
    "scripts": {
        "start": "PORT=3000 react-scripts start",
        "build-frontend": "react-scripts build",
        "test-frontend": "react-scripts test",
        "eject": "react-scripts eject",
        "build": "yarn run installZkutil && yarn run buildVerifyEpochKeySnark && yarn run buildUserStateTransitionSnark && yarn run buildProveReputationSnark && yarn run buildProveReputationFromAttesterSnark && npx hardhat compile && npx ts-node ./src/scripts/buildPoseidon.ts",
        "installZkutil": "./src/scripts/installZkutil.sh",
        "buildVerifyEpochKeySnark": "./src/scripts/buildVerifyEpochKeySnark.sh",
        "buildUserStateTransitionSnark": "./src/scripts/buildUserStateTransitionSnark.sh",
        "buildProveReputationSnark": "./src/scripts/buildProveReputationSnark.sh",
        "buildProveReputationFromAttesterSnark": "./src/scripts/buildProveReputationFromAttesterSnark.sh",
        "test-cli": "npx hardhat compile && npx ts-node ./src/scripts/buildPoseidon.ts && ./src/scripts/testCLI.sh",
        "test": "npx hardhat compile && npx ts-node ./src/scripts/buildPoseidon.ts && yarn run contractUintTests && yarn run circuitUintTests && yarn run integrationTest",
        "contractUintTests": "NODE_OPTIONS=--max-old-space-size=4096 npx hardhat test --no-compile $(find test/contracts -name '*.ts')",
        "circuitUintTests": "NODE_OPTIONS=--max-old-space-size=4096 npx hardhat test --no-compile $(find test/circuits -name '*.ts')",
        "integrationTest": "npx ts-node scripts/buildPoseidon.ts && NODE_OPTIONS=--max-old-space-size=4096 npx hardhat test --no-compile $(find test/integration -name '*.ts')"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vivianjeng/Unirep-Social.git"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.0.2",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@types/chai": "^4.2.14",
        "@types/mocha": "^8.0.4",
        "@types/node": "^15.12.4",
        "chai": "^4.2.0",
        "ethereum-waffle": "^3.3.0",
        "hardhat": "^2.3.0",
        "keyv": "^4.0.3",
        "ts-node": "^9.0.0"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^3.2.0",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.170",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.8",
        "argparse": "^2.0.1",
        "babel-loader": "8.1.0",
        "base64url": "^3.0.1",
        "bootstrap": "^5.0.1",
        "circom": "0.5.38",
        "circomlib": "0.5.1",
        "ethers": "^5.2.0",
        "html-webpack-plugin": "^5.3.2",
        "libsemaphore": "^1.0.16",
        "lodash": "^4.17.21",
        "maci-config": "^0.7.4",
        "maci-crypto": "^0.7.4",
        "mongoose": "^5.12.10",
        "n-readlines": "^1.0.1",
        "node-sass": "^6.0.1",
        "postcss-loader": "^5.3.0",
        "prompt-async": "^0.9.9",
        "react": "^17.0.2",
        "react-app-rewired": "^2.1.8",
        "react-bootstrap": "^1.6.1",
        "react-dom": "^17.0.2",
        "react-icons": "^4.2.0",
        "react-refresh": "^0.10.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "shelljs": "^0.8.3",
        "snarkjs": "0.3.59",
        "source-map-loader": "^3.0.0",
        "style-loader": "^2.0.0",
        "ts-loader": "^9.2.3",
        "typescript": "^4.0.5",
        "web-vitals": "^2.0.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
