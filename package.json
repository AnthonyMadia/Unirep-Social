{
    "name": "unirep-social",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "start": "nodemon src/app.ts",
        "build": "yarn run installZkutil && yarn run buildVerifyEpochKeySnark && yarn run buildUserStateTransitionSnark && yarn run buildProveReputationSnark && yarn run buildProveReputationFromAttesterSnark && yarn compile",
        "compile": "npx hardhat compile && yarn run moveArtifacts && npx ts-node ./src/scripts/buildPoseidon.ts",
        "installZkutil": "./src/scripts/installZkutil.sh",
        "buildVerifyEpochKeySnark": "./src/scripts/buildVerifyEpochKeySnark.sh",
        "buildUserStateTransitionSnark": "./src/scripts/buildUserStateTransitionSnark.sh",
        "buildProveReputationSnark": "./src/scripts/buildProveReputationSnark.sh",
        "buildProveReputationFromAttesterSnark": "./src/scripts/buildProveReputationFromAttesterSnark.sh",
        "moveArtifacts": "./src/scripts/moveArtifacts.sh",
        "test-cli": "./src/scripts/testCLI.sh",
        "test": "yarn run contractUintTests && yarn run circuitUintTests && yarn run integrationTest",
        "contractUintTests": "NODE_OPTIONS=--max-old-space-size=4096 npx hardhat test --no-compile $(find ./src/test/contracts -name '*.ts')",
        "circuitUintTests": "NODE_OPTIONS=--max-old-space-size=4096 npx hardhat test --no-compile $(find ./src/test/circuits -name '*.ts')",
        "integrationTest": " NODE_OPTIONS=--max-old-space-size=4096 npx hardhat test --no-compile $(find ./src/test/integration -name '*.ts')"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vivianjeng/Unirep-Social.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vivianjeng/Unirep-Social/issues"
    },
    "homepage": "https://github.com/vivianjeng/Unirep-Social#readme",
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.0.2",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@types/chai": "^4.2.14",
        "@types/express": "^4.17.13",
        "@types/mocha": "^8.0.4",
        "@types/node": "^16.3.3",
        "chai": "^4.2.0",
        "ethereum-waffle": "^3.3.0",
        "hardhat": "^2.3.0",
        "keyv": "^4.0.3",
        "nodemon": "^2.0.12",
        "ts-node": "^9.0.0"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^3.2.0",
        "argparse": "^2.0.1",
        "base64url": "^3.0.1",
        "circom": "^0.5.38",
        "circomlib": "^0.5.1",
        "dotenv": "^10.0.0",
        "ethers": "^5.2.0",
        "express": "^4.17.1",
        "libsemaphore": "^1.0.15",
        "maci-config": "^0.7.4",
        "maci-crypto": "^0.7.4",
        "mongoose": "^5.12.10",
        "n-readlines": "^1.0.1",
        "prompt-async": "^0.9.9",
        "shelljs": "^0.8.3",
        "snarkjs": "^0.3.59",
        "typescript": "^4.0.5"
    }
}
